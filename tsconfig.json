{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "es6"
    ],
    "module": "commonjs",
    "jsx": "react",
    "noImplicitAny": true,
    "allowJs": false,
    "moduleResolution": "node",
    "isolatedModules": false,
    "declaration": true,

    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noEmit": false
  }
}
